{
	"languageserver": {
		"ccls": {
			"trace.server": "verbose",
			"command": "ccls",
			"filetypes": [
				"c",
				"cpp",
				"objc",
				"objcpp"
			],
			"rootPatterns": [
				".ccls",
				"compile_commands.json",
				".git/",
				".hg/"
			],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				},
				"compilationDatabaseDirectory": "build"
			}
		},
		"golang": {
			"command": "gopls",
			"rootPatterns": [
				"go.mod",
				".vim/",
				".git/",
				".hg/"
			],
			"filetypes": [
				"go"
			],
			"initializationOptions": {
				"usePlaceholders": true
			}
		},
		"cmake": {
			"command": "/home/jz/.local/lib/python3.6/site-packages/cmake_language_server/server.py",
			"filetypes": [
				"cmake"
			],
			"rootPatterns": [
				"build/"
			],
			"initializationOptions": {
				"buildDirectory": "build"
			}
		}
	},
	"coc.preferences.extensionUpdateCheck": "never",
	"suggest.noselect": false,
}
